{
    "$schema": "https://vega.github.io/schema/vega-lite/v5.json",
    "description": "GHG emissions",
    "data": {
        "url": "https://raw.githubusercontent.com/willdiaz635/willdiaz635.github.io/main/KyotoGHGEmissions.csv"
    },
    "title": {
        "text": "Greenhouse Gas emissions (1850-2018)",
        "subtitle": "Source: Climatewatch.com - PIK"
    },
    "height": 450,
    "width": 750,
    "mark": "line",
    "encoding": {
        "x": {
            "field": "Year",
            "type": "temporal"
        },
        "y": {
            "field": "Pollution",
            "type": "quantitative"
        },
        "color": {
            "field": "Category",
            "type": "nominal"
        }
    }
}